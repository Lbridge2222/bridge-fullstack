# models.py\
from pydantic import BaseModel\
from datetime import datetime\
from typing import Optional\
\
class Contact(BaseModel):\
    contact_id: int\
    create_date: datetime\
    confirmed_course: Optional[str]\
    campus_location: Optional[str]\
    offer_status: Optional[str]\
    counted: Optional[str]\
    cycle_year: Optional[int]\
    clean_lead_source: Optional[str]\
    has_offer: Optional[bool]\
    has_accepted: Optional[bool]\
    is_enrolled: Optional[bool]\
    \
    class Config:\
        from_attributes = True\
\
# api.py\
@app.get("/contacts", response_model=List[Contact])\
async def get_contacts(\
    limit: int = 100,\
    offset: int = 0,\
    course: Optional[str] = None,\
    campus: Optional[str] = None\
):\
    query = select(contacts_table)\
    if course:\
        query = query.where(contacts_table.c.confirmed_course == course)\
    if campus:\
        query = query.where(contacts_table.c.campus_location == campus)\
    \
    result = await database.fetch_all(query.limit(limit).offset(offset))\
    return result\
        </div>\
