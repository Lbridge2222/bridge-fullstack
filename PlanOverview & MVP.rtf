// components/ContactList.tsx\
'use client'\
import \{ useQuery \} from '@tanstack/react-query'\
import \{ Contact \} from '@/types/contact'\
\
interface ContactListProps \{\
  course?: string\
  campus?: string\
\}\
\
export function ContactList(\{ course, campus \}: ContactListProps) \{\
  const \{ data: contacts, isLoading \} = useQuery(\{\
    queryKey: ['contacts', course, campus],\
    queryFn: () => fetchContacts(\{ course, campus \})\
  \})\
\
  if (isLoading) return <ContactListSkeleton />\
\
  return (\
    <div className="space-y-4">\
      \{contacts?.map((contact) => (\
        <ContactCard key=\{contact.contact_id\} contact=\{contact\} />\
      ))\}\
    </div>\
  )\
\}\
\
// API client\
async function fetchContacts(filters: ContactFilters): Promise<Contact[]> \{\
  const params = new URLSearchParams()\
  if (filters.course) params.set('course', filters.course)\
  if (filters.campus) params.set('campus', filters.campus)\
  \
  const response = await fetch(`/api/contacts?$\{params\}`)\
  return response.json()\
\}\
