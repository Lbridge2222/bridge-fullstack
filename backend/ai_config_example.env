# AI Configuration Example
# Copy this to .env and update with your actual API keys

# AI Feature Flags
AI_PARSER_ENABLED=true
AI_NARRATOR_ENABLED=true
AI_TIMEOUT_MS=2200
AI_CACHE_TTL_S=900
AI_NORMALISER_MODEL=gemini-2.0-flash
AI_PARSER_MODEL=gemini-2.0-flash
AI_NARRATOR_MODEL=gemini-2.0-flash

# Ivy v2 Dual Mode Flags - GA Configuration
IVY_ORGANIC_ENABLED=true
IVY_MODAL_HEURISTICS=false
ACTIVE_MODEL=gemini-2.0-flash

# Staged Cutover Flags
APP_ENV=production
GEMINI_MODEL=gemini-2.0-flash
FALLBACK_MODELS=gemini-2.0-flash-latest,gemini-2.0-flash-exp

# Circuit Breaker Settings
CIRCUIT_BREAKER_ERROR_THRESHOLD=5
CIRCUIT_BREAKER_TIME_WINDOW=30
CIRCUIT_BREAKER_RECOVERY_TIME=120

# Rate Limiting
AI_RATE_LIMIT_PER_MIN=60
AI_BURST_LIMIT=10

# Existing AI Configuration
AI_LEADS_ENABLED=true
AI_MODEL_PROVIDER=gemini
GEMINI_API_KEY=your_gemini_api_key_here
GEMINI_MODEL=gemini-2.0-flash
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-4o-mini
